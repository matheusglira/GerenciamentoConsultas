@model IEnumerable<GerenciamentoConsultas.Models.TipoExame>

@{
    ViewBag.Title = "List";
}

<h2>Tipos de Exame</h2>

<p>
    @Html.ActionLink("Cadastrar um novo tipo de Exame", "Create", "CadastroTipoExames", new { @class = "btn btn-success" })
</p>

<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(m => m.NmTipoExame) </th>
        <th>@Html.DisplayNameFor(m => m.Descricao)</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.NmTipoExame)</td>
            <td>@Html.DisplayFor(modelItem => item.Descricao)</td>
            <td>
                @Html.ActionLink("Editar", "Editar", new { id = item.TipoExameId }) |
                @Html.ActionLink("Detalhes", "Details", new { id = item.TipoExameId }) |
                @Html.ActionLink("Deletar", "Delete", new { id = item.TipoExameId })
            </td>
        </tr>
    }
</table>